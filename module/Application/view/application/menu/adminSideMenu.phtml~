<script type="text/javascript">
var pthName = window.location.pathname;
var serPrm = window.location.search;
var selectorr= "/" +pthName.substr(1);

if(window.location.search == ''){
	var ssdiv = $('a[href="' + selectorr + '"]').closest('.panel-collapse');
	ssdiv.toggleClass("panel-collapse in");
	
}else{
	selectorr = pthName+serPrm;
	var ssdiv = $('a[href="' + selectorr + '"]').closest('.panel-collapse');
	ssdiv.toggleClass("panel-collapse in");
	
}

//$( "li.item-a" ).parent().css( "background-color", "red" );
</script>
